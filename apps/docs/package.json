{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vitepress dev",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vitepress build",
    "preview": "vitepress preview",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path ../../.gitignore --fix src",
    "test:lint": "eslint --ext .js,.ts,.vue --ignore-path ../../.gitignore src",
    "test": "pnpm run test:lint"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.1.17",
    "@iconify-json/simple-icons": "^1.1.55",
    "@rushstack/eslint-patch": "^1.3.0",
    "@tsconfig/node20": "^1.0.1",
    "@types/node": "^20.2.5",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.1.2",
    "bootstrap": "^5.3.0",
    "bootstrap-vue-next": "workspace:^",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-define-config": "^1.20.0",
    "eslint-plugin-vue": "^9.14.1",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "unplugin-icons": "^0.16.2",
    "vitepress": "1.0.0-beta.1",
    "vue": "^3.3.4"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix",
    "*": "prettier --write --ignore-unknown"
  }
}
