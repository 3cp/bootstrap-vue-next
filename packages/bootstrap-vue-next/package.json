{
  "name": "bootstrap-vue-next",
  "displayName": "BootstrapVueNext",
  "description": "Early (but lovely) implementation of Vue 3, Bootstrap 5 and Typescript",
  "version": "0.7.3",
  "license": "MIT",
  "main": "./dist/bootstrap-vue-next.umd.js",
  "module": "./dist/bootstrap-vue-next.es.js",
  "exports": {
    ".": {
      "import": "./dist/bootstrap-vue-next.es.js",
      "require": "./dist/bootstrap-vue-next.umd.js"
    },
    "./dist/bootstrap-vue-next.css": "./dist/bootstrap-vue-next.css",
    "./nuxt": "./nuxt.ts",
    "./src/styles/styles.scss": "./src/styles/styles.scss"
  },
  "files": [
    "dist",
    "src",
    "nuxt.ts",
    "!src/**/*.spec.ts",
    "!src/**/*.spec.js",
    "!src/App.vue",
    "!src/main.ts"
  ],
  "types": "./dist/BootstrapVue.d.ts",
  "private": false,
  "scripts": {
    "dev": "vite --port 5174",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "serve": "vite preview",
    "build": "pnpm run type-check && pnpm run build-only",
    "lint": "eslint --ext .js,.vue --ignore-path ../../.gitignore --fix src",
    "test:lint": "eslint --ext .js,.vue --ignore-path ../../.gitignore src",
    "test:unit": "vitest",
    "test:coverage": "vitest --coverage",
    "test": "pnpm run test:lint && pnpm run test:unit"
  },
  "peerDependencies": {
    "bootstrap": "^5.2.3",
    "vue": "^3.2.45"
  },
  "dependencies": {
    "@floating-ui/vue": "^0.2.1",
    "@nuxt/kit": "3.0.0",
    "@vueuse/core": "^9.11.1"
  },
  "devDependencies": {
    "@floating-ui/core": "^1.2.2",
    "@popperjs/core": "2.11.6",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/bootstrap": "^5.2.6",
    "@types/node": "^18.15.3",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.25.3",
    "@vue/compiler-dom": "^3.2.45",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/runtime-core": "^3.2.45",
    "@vue/shared": "^3.2.45",
    "@vue/test-utils": "^2.2.5",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/shared": "^9.11.1",
    "bootstrap": "^5.2.3",
    "c8": "^7.7.0",
    "csstype": "^3.1.1",
    "eslint": "^8.29.0",
    "eslint-define-config": "^1.17.0",
    "eslint-plugin-vue": "^9.8.0",
    "happy-dom": "^7.7.2",
    "jsdom": "^20.0.3",
    "prettier": "^2.8.0",
    "rollup": "^3.5.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.56.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.25.3",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bootstrap-vue/bootstrap-vue-next.git"
  },
  "bugs": {
    "url": "https://github.com/bootstrap-vue/bootstrap-vue-next/issues"
  },
  "homepage": "https://github.com/bootstrap-vue/bootstrap-vue-next",
  "keywords": [
    "vue3",
    "vue",
    "bootstrap",
    "components",
    "typescript",
    "component-library",
    "bootstrapvue"
  ],
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix",
    "*": "prettier --write --ignore-unknown"
  }
}
